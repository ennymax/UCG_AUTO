            <class name="com.Module.LoginWithCorrectDetails"/>
            <class name="com.Module.LoginWithWrongDomain"/>
            <class name="com.Module.LoginWithWrongPassword"/>
            <class name="com.Module.LoginWithWrongEmail"/>
